  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.prod.json",
    "test": "vitest",
    "lint": "eslint '*/**/*.ts' --ignore-path .gitignore --quiet --fix",
    "format": "./node_modules/.bin/prettier --write --parser typescript --config .prettierrc ."
